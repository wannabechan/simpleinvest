{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "",
  "functions": {
    "api/**/*.js": {
      "includeFiles": "node_modules/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
